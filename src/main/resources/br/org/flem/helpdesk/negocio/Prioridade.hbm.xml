<hibernate-mapping>
    <class name="br.org.flem.helpdesk.negocio.Prioridade" table="prioridade">

        <id name="id" column="id_prioridade" type="integer">
		<generator class="identity" />
	</id>

	<property name="sigla" column="sigla" type="string" unique="true" />
        
        <property name="descricao" column="descricao" type="string" />
        
        
    </class>
</hibernate-mapping>
